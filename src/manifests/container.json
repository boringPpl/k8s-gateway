{
  "image": "jupyter/minimal-notebook",
  "imagePullPolicy": "IfNotPresent",
  "name": "notebook",
  "ports": [{ "containerPort": 8888 }],
  "command": ["tini", "-g", "--", "jupyter", "notebook"],
  "readinessProbe": {
    "initialDelaySeconds": 2,
    "periodSeconds": 2,
    "successThreshold": 1,
    "httpGet": {
      "path": "/api"
    }
  }
}
